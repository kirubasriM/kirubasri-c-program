bool checkRecord(char* s) {
    int ac=0,lc=0;
 for(int i=0;i<strlen(s);i++)
 {
    if(s[i]=='A')
    {
        ac++;
    }
 
    if(ac>1)
    {
return false;
    }
 }
 for(int i=0;i<strlen(s);i++){
    if(s[i]=='L')
    {
lc++;
if(lc>=3){
    return false;  
 }
    }
 else{
    lc=0;
 }

 }
return true;
 }
